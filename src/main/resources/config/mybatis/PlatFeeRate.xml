<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmrz.mybatis.platFeeRate">
	<select id="getPlatFeeRate"  parameterType="String" resultType="String">
		SELECT
			spfr.rate
		FROM
			s_plat_fee_rate spfr
		where spfr.is_del = 0 and spfr.type = #{type}
	</select>
</mapper>