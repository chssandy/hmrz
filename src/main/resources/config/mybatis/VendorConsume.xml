<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hmrz.mybatis.vendorConsume">

	<insert id="addVendorConsume" parameterType="VendorConsumeBean" useGeneratedKeys="true" keyProperty="consume_id">
		insert into C_VENDOR_CONSUME(
			vendor_id,task_type,task_consume,plat_consume,task_id,execute_id,task_name) 
		values(#{vendor_id},#{task_type},#{task_consume},#{plat_consume},#{task_id},#{execute_id},#{task_name})
	</insert>

</mapper>